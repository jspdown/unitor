  },
  "author": "Jean-Baptiste Louazel <jb.louazel@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "mocha": "^1.21.4",
    "istanbul": "^0.3.2",
    "jshint": "^2.5.6",
    "jscs": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jlouazel/unitor.git"
  }
}
