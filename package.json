{
  "name": "unitor",
  "description": "A lightweight module to manipulate all the units you want.",
  "version": "0.5.0",
  "dependencies": {
    "lodash": "^2.4.1"
  },
  "scripts": {
    "lint": "jshint .",
    "checkStyle": "jscs .",
    "pretest": "npm run-script lint && npm run-script checkStyle",
